#!/bin/bash
docker build -f src/main/docker/Dockerfile.jvm -t chat-integracao:$1 .

docker build -f src/main/docker/Dockerfile.jvm -t chat-integracao:latest .

docker build -f src/main/docker/Dockerfile.jvm -t registry.truesystem.com.br/chat-integracao:$1 .

docker build -f src/main/docker/Dockerfile.jvm -t registry.truesystem.com.br/chat-integracao:latest .